# Alternative GNU Make workspace makefile autogenerated by Premake

ifndef config
  config=debug
endif

ifndef verbose
  SILENT = @
endif

ifeq ($(config),debug)
  Taller08_config = debug

else ifeq ($(config),release)
  Taller08_config = release

else
  $(error "invalid configuration $(config)")
endif

PROJECTS := Taller08

.PHONY: all clean help $(PROJECTS) 

all: $(PROJECTS)

Taller08:
ifneq (,$(Taller08_config))
	@echo "==== Building Taller08 ($(Taller08_config)) ===="
	@${MAKE} --no-print-directory -C . -f Taller08.make config=$(Taller08_config)
endif

clean:
	@${MAKE} --no-print-directory -C . -f Taller08.make clean

help:
	@echo "Usage: make [config=name] [target]"
	@echo ""
	@echo "CONFIGURATIONS:"
	@echo "  debug"
	@echo "  release"
	@echo ""
	@echo "TARGETS:"
	@echo "   all (default)"
	@echo "   clean"
	@echo "   Taller08"
	@echo ""
	@echo "For more information, see https://github.com/premake/premake-core/wiki"