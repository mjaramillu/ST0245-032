# Alternative GNU Make workspace makefile autogenerated by Premake

ifndef config
  config=debug
endif

ifndef verbose
  SILENT = @
endif

ifeq ($(config),debug)
  Taller09_config = debug

else ifeq ($(config),release)
  Taller09_config = release

else
  $(error "invalid configuration $(config)")
endif

PROJECTS := Taller09

.PHONY: all clean help $(PROJECTS) 

all: $(PROJECTS)

Taller09:
ifneq (,$(Taller09_config))
	@echo "==== Building Taller09 ($(Taller09_config)) ===="
	@${MAKE} --no-print-directory -C . -f Taller09.make config=$(Taller09_config)
endif

clean:
	@${MAKE} --no-print-directory -C . -f Taller09.make clean

help:
	@echo "Usage: make [config=name] [target]"
	@echo ""
	@echo "CONFIGURATIONS:"
	@echo "  debug"
	@echo "  release"
	@echo ""
	@echo "TARGETS:"
	@echo "   all (default)"
	@echo "   clean"
	@echo "   Taller09"
	@echo ""
	@echo "For more information, see https://github.com/premake/premake-core/wiki"