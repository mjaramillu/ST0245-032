# Alternative GNU Make workspace makefile autogenerated by Premake

ifndef config
  config=debug
endif

ifndef verbose
  SILENT = @
endif

ifeq ($(config),debug)
  Taller06_config = debug

else ifeq ($(config),release)
  Taller06_config = release

else
  $(error "invalid configuration $(config)")
endif

PROJECTS := Taller06

.PHONY: all clean help $(PROJECTS) 

all: $(PROJECTS)

Taller06:
ifneq (,$(Taller06_config))
	@echo "==== Building Taller06 ($(Taller06_config)) ===="
	@${MAKE} --no-print-directory -C . -f Taller06.make config=$(Taller06_config)
endif

clean:
	@${MAKE} --no-print-directory -C . -f Taller06.make clean

help:
	@echo "Usage: make [config=name] [target]"
	@echo ""
	@echo "CONFIGURATIONS:"
	@echo "  debug"
	@echo "  release"
	@echo ""
	@echo "TARGETS:"
	@echo "   all (default)"
	@echo "   clean"
	@echo "   Taller06"
	@echo ""
	@echo "For more information, see https://github.com/premake/premake-core/wiki"