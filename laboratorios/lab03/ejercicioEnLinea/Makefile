# Alternative GNU Make workspace makefile autogenerated by Premake

ifndef config
  config=debug
endif

ifndef verbose
  SILENT = @
endif

ifeq ($(config),debug)
  Lab03_config = debug

else ifeq ($(config),release)
  Lab03_config = release

else
  $(error "invalid configuration $(config)")
endif

PROJECTS := Lab03

.PHONY: all clean help $(PROJECTS) 

all: $(PROJECTS)

Lab03:
ifneq (,$(Lab03_config))
	@echo "==== Building Lab03 ($(Lab03_config)) ===="
	@${MAKE} --no-print-directory -C . -f Lab03.make config=$(Lab03_config)
endif

clean:
	@${MAKE} --no-print-directory -C . -f Lab03.make clean

help:
	@echo "Usage: make [config=name] [target]"
	@echo ""
	@echo "CONFIGURATIONS:"
	@echo "  debug"
	@echo "  release"
	@echo ""
	@echo "TARGETS:"
	@echo "   all (default)"
	@echo "   clean"
	@echo "   Lab03"
	@echo ""
	@echo "For more information, see https://github.com/premake/premake-core/wiki"